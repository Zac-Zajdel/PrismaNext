{
  "name": "prisma-next",
  "version": "0.1.0",
  "description": "Next.js + Prisma + Typescript + Tailwind CSS + ES Lint + Husky + Prettier starter template",
  "author": "Zachary Zajdel <github.com/Zac-Zajdel>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zac-Zajdel/PrismaNext.git"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "tailwindcss",
    "prisma",
    "pwa",
    "eslint",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "eslint --ignore-path .gitignore \"*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"*.+(ts|js|tsx)\" --write",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Write tests!\" && exit 0",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "lint-staged": {
    "*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.0.0",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "next": "^13.0.6",
    "next-auth": "^4.10.3",
    "next-pwa": "^5.5.4",
    "postcss": "^8.4.5",
    "prisma": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@types/node": "^18.11.15",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "8.34.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "embeddedLanguageFormatting": "off",
    "htmlWhitespaceSensitivity": "strict"
  }
}
